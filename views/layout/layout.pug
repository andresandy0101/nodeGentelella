include ../../vendor/gantelella/grid
include ../../vendor/gantelella/form

doctype html
html(lang="en")
	head
		meta(http-equiv="Content-Type" content="text/html; charset=UTF-8")
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport" content="width=device-width, initial-scale=1")

		title= title

		// Bootstrap
		link(href="/gantelella/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet")
		// Font Awesome
		link(href="/gantelella/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet")
		// NProgress
		link(href="/gantelella/vendors/nprogress/nprogress.css" rel="stylesheet")
		// iCheck
		link(href="/gantelella/vendors/iCheck/skins/flat/green.css" rel="stylesheet")

		// bootstrap-progressbar
		link(href="/gantelella/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet")
		// JQVMap
		link(href="/gantelella/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet")
		// bootstrap-daterangepicker
		link(href="/gantelella/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet")

		link(href="/gantelella/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet")
		link(href="/gantelella/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet")
		link(href="/gantelella/vendors/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet")
		link(href="/gantelella/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet")
		link(href="/gantelella/vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet")
		link(href="/gantelella/vendors/bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css" rel="stylesheet")

		// Custom Theme Style
		link(href="/gantelella/build/css/custom.min.css" rel="stylesheet")

	body(class="nav-md")
		div(class="container body")
			div(class="main_container")
				//left menu
				div(class="col-md-3 left_col")
					div(class="left_col scroll-view")
						div(class="navbar nav_title" style="border: 0;")
							a(href="/" class="site_title")
								i(class="fa fa-paw")
								span 
									!= 'Gentelella Alela!'

						div(class="clearfix")
						// menu profile quick info
						div(class="profile clearfix")
							div(class="profile_pic")
								img(src="/gantelella/production/images/img.jpg" alt="..." class="img-circle profile_img")
							div(class="profile_info")
								span
									!= 'Welcome'
								h2
									!= 'John Doe'
						// menu profile quick info
						|
						// sidebar menu
						div(id="sidebar-menu" class="main_menu_side hidden-print main_menu")
							div(class="menu_section")
								h3 
									!= 'General'
								ul(class="nav side-menu")
									include left.pug
						// sidebar menu

						// menu footer buttons
						div(class="sidebar-footer hidden-small")
							a(data-toggle="tooltip" data-placement="top" title="Settings")
								span(class="glyphicon glyphicon-cog" aria-hidden="true")
							a(data-toggle="tooltip" data-placement="top" title="FullScreen")
								span(class="glyphicon glyphicon-fullscreen" aria-hidden="true")
							a(data-toggle="tooltip" data-placement="top" title="Lock")
								span(class="glyphicon glyphicon-eye-close" aria-hidden="true")
							a(data-toggle="tooltip" data-placement="top" title="Logout" href="/login")
								span(class="glyphicon glyphicon-off" aria-hidden="true")
						// menu footer buttons
				//left menu

				//top navigation
				div(class="top_nav")
					div(class="nav_menu")
						nav
							div(class="nav toggle")
								a(id="menu_toggle")
									i(class="fa fa-bars")

							ul(class="nav navbar-nav navbar-right")
								li
									a(href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false")
										img(src="/gantelella/production/images/img.jpg" alt="") 
										!= 'John Doe'
										span(class=" fa fa-angle-down")
									ul(class="dropdown-menu dropdown-usermenu pull-right")
										li
											a(href="javascript:;")
												!= 'Profile'
										li
											a(href="javascript:;")
												span
													!='Settings'
										li
											a(href="javascript:;") 
												!='Help'
										li
											a(href="/login")
												i(class="fa fa-sign-out pull-right") 
												!= 'Log Out'

								li(role="presentation" class="dropdown")
									a(href="javascript:;" class="dropdown-toggle info-number" data-toggle="dropdown" aria-expanded="false")
										i(class="fa fa-envelope-o")
										span(class="badge bg-green")
											!= '1'
									ul(id="menu1" class="dropdown-menu list-unstyled msg_list" role="menu")
										li
											a
												span(class="image")
													img(src="/gantelella/production/images/img.jpg" alt="Profile Image")
												span
													span
														!= 'John Smith'
													span(class="time")
														!= '3 mins ago'
												span(class="message")
													!= 'Film festivals used to be do-or-die moments for movie makers. They were where...'
										li
											div(class="text-center")
											a
												!= '<strong>See All Alerts</strong>'
												i(class="fa fa-angle-right")
				//top navigation

				//page content
				div(class="right_col" role="main")
					div
						div(class="col-md-12 col-sm-12 col-xs-12")
							div(class="x_panel")
								div(class="x_title")
									h2= title
									div(class="clearfix")
								div(class="x_content")
									block content

				// footer content
				footer
					div(class="pull-right")
						!= 'Gentelella - Bootstrap Admin Template by '
						a(href="https://colorlib.com") 
							!= 'Colorlib'
					div(class="clearfix")

		// jQuery 
		script(src="/gantelella/vendors/jquery/dist/jquery.min.js")
		// Bootstrap 
		script(src="/gantelella/vendors/bootstrap/dist/js/bootstrap.min.js")
		// FastClick 
		script(src="/gantelella/vendors/fastclick/lib/fastclick.js")
