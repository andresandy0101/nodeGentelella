		script(src="/gantelella/vendors/nprogress/nprogress.js")
		// Chart.js 
		script(src="/gantelella/vendors/Chart.js/dist/Chart.min.js")
		// jQuery Sparklines 
		script(src="/gantelella/vendors/jquery-sparkline/dist/jquery.sparkline.min.js")
		// Flot 
		script(src="/gantelella/vendors/Flot/jquery.flot.js")
		script(src="/gantelella/vendors/Flot/jquery.flot.pie.js")
		script(src="/gantelella/vendors/Flot/jquery.flot.time.js")
		script(src="/gantelella/vendors/Flot/jquery.flot.stack.js")
		script(src="/gantelella/vendors/Flot/jquery.flot.resize.js")
		// Flot plugins 
		script(src="/gantelella/vendors/flot.orderbars/js/jquery.flot.orderBars.js")
		script(src="/gantelella/vendors/flot-spline/js/jquery.flot.spline.min.js")
		script(src="/gantelella/vendors/flot.curvedlines/curvedLines.js")
		// DateJS 
		script(src="/gantelella/vendors/DateJS/build/date.js")
		// bootstrap-daterangepicker 
		script(src="/gantelella/vendors/moment/min/moment.min.js")
		script(src="/gantelella/vendors/bootstrap-daterangepicker/daterangepicker.js")

		// Custom Theme Scripts 
		script(src="/gantelella/build/js/custom.min.js")
		// gauge.js 
		script(src="/gantelella/vendors/gauge.js/dist/gauge.min.js")
		// bootstrap-progressbar 
		script(src="/gantelella/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js")
		// iCheck 
		script(src="/gantelella/vendors/iCheck/icheck.min.js")
		// Skycons 
		script(src="/gantelella/vendors/skycons/skycons.js")
		// JQVMap 
		script(src="/gantelella/vendors/jqvmap/dist/jquery.vmap.js")
		script(src="/gantelella/vendors/jqvmap/dist/maps/jquery.vmap.world.js")
		script(src="/gantelella/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js")
		// morris.js 
		script(src="/gantelella/vendors/raphael/raphael.min.js")
		script(src="/gantelella/vendors/morris.js/morris.min.js")

		//Sweetalert2
		script(src="/sweetalert2/dist/sweetalert2.all.min.js")

		script(src="/gantelella/vendors/datatables.net/js/jquery.dataTables.min.js")
		script(src="/gantelella/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js")
		script(src="/gantelella/vendors/datatables.net-buttons/js/dataTables.buttons.min.js")
		script(src="/gantelella/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js")
		script(src="/gantelella/vendors/datatables.net-buttons/js/buttons.flash.min.js")
		script(src="/gantelella/vendors/datatables.net-buttons/js/buttons.html5.min.js")
		script(src="/gantelella/vendors/datatables.net-buttons/js/buttons.print.min.js")
		script(src="/gantelella/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js")
		script(src="/gantelella/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js")
		script(src="/gantelella/vendors/datatables.net-responsive/js/dataTables.responsive.min.js")
		script(src="/gantelella/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js")
		script(src="/gantelella/vendors/datatables.net-scroller/js/dataTables.scroller.min.js")
		script(src="/gantelella/vendors/jszip/dist/jszip.min.js")
		script(src="/gantelella/vendors/pdfmake/build/pdfmake.min.js")
		script(src="/gantelella/vendors/pdfmake/build/vfs_fonts.js")
		script(src="/gantelella/vendors/bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js")
		script(src="/gantelella/vendors/validator/validator.js")

		if alert
			script.
				Swal(!{JSON.stringify(alert)});

		script.
			$('#myDatepicker').datetimepicker({format: 'YYYY-MM-DD HH:mm'});
    
			$('#myDatepicker2').datetimepicker({ format: 'YYYY-MM-DD' });
    
			$('#myDatepicker3').datetimepicker({ format: 'HH:mm' });


		script.
			function confirmaDelete(url){
				Swal({
					title: 'Are you sure?',
					text: "You won't be able to revert this!",
					type: 'warning',
					showCancelButton: true,
					confirmButtonColor: '#3085d6',
					cancelButtonColor: '#d33',
					confirmButtonText: 'Yes, delete it!'
				}).then((result) => {
					if (result.value)
						window.location.href = url;
				});
			}



